<!DOCTYPE html>
<html>
<head>
    <title><%=settings.title%></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
</head>
<body>
<h1><%=settings.character%></h1>

<p>Express stock photo application.</p>

<div id="photos">
    <% photos.forEach(function(photo){ %>
    <div class="photo">
        <h2><%= photo.name %></h2>
        <a href='/images/<%=photo.id%>/download'>
            <img src='/images/<%= photo.path %>'/>
        </a>
    </div>
    <% }) %>
</div>
</body>
</html>
